{
  "name": "template",
  "version": "1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "yarn-clean": "yarn cache clean && react-native start --reset-cache",
    "rn-install": "rm -rf node_modules && yarn install && cd ios && rm -rf Podfile.lock && rm -rf Pods && pod install && cd ..",
    "android-build": "cd android && ./gradlew clean && ./gradlew assembleRelease && ./gradlew bundleRelease && cd ..",
    "android-clean": "cd android && rm -rf build && ./gradlew clean && cd ..",
    "test:all": "yarn tsc --noEmit && yarn test:eslint",
    "test:all:fix": "yarn tsc --noEmit && yarn test:eslint:fix && yarn test:prettier",
    "test:eslint": "eslint './src/**/*.{ts,tsx,js}'",
    "test:eslint:fix": "eslint --fix './src/**/*.{ts,tsx,js}'",
    "test:prettier": "prettier --write './src/**/*.{js,ts,tsx,json,md}'",
    "test:commit": "commitlint -e"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "pluralize-ru": "^1.0.1",
    "react": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-native": "0.69.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-mmkv": "^2.4.3",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.3.0",
    "react-native-webp-format": "^1.1.2",
    "react-query": "^3.39.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^3.0.3",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-dirs": "^0.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.71.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^18"
  }
}
